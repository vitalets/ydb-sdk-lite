{
  "name": "ydb-sdk-lite",
  "description": "Lightweight implementation of Yandex Database SDK for Node.js",
  "version": "1.5.1",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist",
    "proto",
    "README.md"
  ],
  "scripts": {
    "init": "git config core.hooksPath ./.hooks",
    "lint": "eslint src test --ext .ts",
    "test": "mocha -r ts-node/register test/setup.ts test/specs/*.ts",
    "test:d": "DEBUG=ydb-sdk-lite:* npm t",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "pull-proto": "./scripts/pull-proto.sh",
    "compile-proto": "./scripts/compile-proto.sh",
    "release": "npm run lint && npm t && npm run build && npm version $VER && npm publish && git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.4.1",
    "debug": "^4.3.2",
    "protobufjs": "^6.11.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-plugin-mocha": "^9.0.0",
    "lint-staged": "^11.2.3",
    "mocha": "^9.1.3",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4",
    "yandex-cloud-lite": "^1.2.2"
  },
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/ydb-sdk-lite.git"
  },
  "license": "MIT"
}
