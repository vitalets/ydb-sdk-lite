{
  "name": "ydb-sdk-lite",
  "description": "Lightweight implementation of Yandex Database SDK for Node.js",
  "version": "1.2.0",
  "main": "dist/index.js",
  "files": [
    "dist",
    "proto",
    "README.md"
  ],
  "scripts": {
    "init": "git config core.hooksPath ./.hooks",
    "lint": "eslint src test --ext .ts",
    "test": "mocha -r ts-node/register test/setup.ts test/specs/*.ts",
    "test:d": "DEBUG=ydb-sdk-lite:* npm t",
    "build": "rm -rf dist && tsc",
    "update-proto": "cp -R node_modules/ydb-sdk/proto ./proto",
    "release": "npm run lint && npm t && npm run build && npm version $VER && npm publish && git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.1",
    "debug": "^4.3.1",
    "protobufjs": "^6.11.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv": "^9.0.2",
    "eslint": "^7.24.0",
    "eslint-plugin-mocha": "^8.1.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "yandex-cloud": "^1.4.3",
    "ydb-sdk": "^1.9.0"
  },
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/ydb-sdk-lite.git"
  },
  "license": "MIT"
}
